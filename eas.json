{
  "cli": {
    "version": ">= 10.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://looton-backend-production.up.railway.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "autoIncrement": false,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://looton-backend-production.up.railway.app"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": false,
      "prebuildCommand": "npx expo prebuild --clean",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://looton-backend-production.up.railway.app"
      }
    },
    "production-apk": {
      "channel": "production",
      "autoIncrement": false,
      "prebuildCommand": "npx expo prebuild --clean",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://looton-backend-production.up.railway.app"
      }
    }
  }
}