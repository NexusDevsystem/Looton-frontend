{
  "cli": {
    "version": ">= 10.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://looton-backend.onrender.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://looton-backend.onrender.com"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "app-bundle",
        "versionCode": 7
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://looton-backend.onrender.com"
      }
    },
    "production-apk": {
      "channel": "production",
      "android": {
        "buildType": "apk",
        "versionCode": 7
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://looton-backend.onrender.com"
      }
    }
  }
}