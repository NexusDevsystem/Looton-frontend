{
  "expo": {
    "name": "Looton",
    "slug": "looton-app",
    "version": "0.1.0",
    "main": "./index.js",
    "orientation": "portrait",
    "scheme": "looton",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "runtimeVersion": "1.0.0",
    "android": {
      "package": "com.nexusdevsystem.looton",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.nexusdevsystem.looton"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-notifications",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "newArchEnabled": false
          },
          "ios": {
            "newArchEnabled": false
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "android_app_id": "ca-app-pub-2976862302591431~7964761364",
          "ios_app_id": "ca-app-pub-2976862302591431~7964761364"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "41306841-8939-4568-a1a1-af93af0428d1"
      }
    },
    "owner": "nyill",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    }
  }
}