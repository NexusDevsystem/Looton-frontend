{
  "expo": {
    "name": "Looton",
    "slug": "looton-app",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "looton",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/favicon.png",
    "splash": {
      "image": "./assets/images/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.nexusdevsystem.looton",
      "runtimeVersion": "1.0.0",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.nexusdevsystem.looton",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          },
          "ios": {
            "newArchEnabled": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "9f504692-ecb1-4a42-9949-15bd6789b1ec"
      }
    },
    "owner": "nexusdevsystem",
    "updates": {
      "url": "https://u.expo.dev/9f504692-ecb1-4a42-9949-15bd6789b1ec"
    }
  }
}
